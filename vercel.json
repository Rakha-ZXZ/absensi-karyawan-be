{
  "version": 2,
  "builds": [
    { 
      "src": "api/server.js", 
      "use": "@vercel/node",
      "config": {
        "includeFiles": [          
          "controllers/**",
          "middlewares/**",         
          "models/**",
          "routes/**",
          "utils/**"
        ]
      }
    }
  ],
  "routes": [
    { 
      "src": "/(.*)", 
      "dest": "api/server.js" 
    }
  ]
}